{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^.*Dockerfile$"],
      "matchStrings": ["^FROM (?<depName>ubuntu):(?<currentValue>[a-zA-Z]+)-(?<build>\\d+)"],
      "depNameTemplate": "{{depName}}",
      "datasourceTemplate": "docker"
    }
  ]
}
